CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE SYNCHRONIZATION_STATUS(SOURCE_SYSTEM_ID VARCHAR(128) NOT NULL,SOURCE_REPOSITORY_ID VARCHAR(128) NOT NULL,TARGET_SYSTEM_ID VARCHAR(128) NOT NULL,TARGET_REPOSITORY_ID VARCHAR(128) NOT NULL,SOURCE_SYSTEM_KIND VARCHAR(128),SOURCE_REPOSITORY_KIND VARCHAR(128),TARGET_SYSTEM_KIND VARCHAR(128),TARGET_REPOSITORY_KIND VARCHAR(128),LAST_SOURCE_ARTIFACT_MODIFICATION_DATE TIMESTAMP,LAST_SOURCE_ARTIFACT_VERSION VARCHAR(10),LAST_SOURCE_ARTIFACT_ID VARCHAR(128),CONFLICT_RESOLUTION_PRIORITY VARCHAR(128),SOURCE_SYSTEM_TIMEZONE VARCHAR(10),TARGET_SYSTEM_TIMEZONE VARCHAR(10),SOURCE_SYSTEM_ENCODING VARCHAR(128),TARGET_SYSTEM_ENCODING VARCHAR(128),PRIMARY KEY(SOURCE_SYSTEM_ID,SOURCE_REPOSITORY_ID,TARGET_SYSTEM_ID,TARGET_REPOSITORY_ID))
CREATE INDEX SYNCHRONIZATION_STATUS_MAPPING ON SYNCHRONIZATION_STATUS(SOURCE_SYSTEM_ID,SOURCE_REPOSITORY_ID,TARGET_SYSTEM_ID,TARGET_REPOSITORY_ID)
CREATE MEMORY TABLE IDENTITY_MAPPING(SOURCE_SYSTEM_ID VARCHAR(128) NOT NULL,SOURCE_REPOSITORY_ID VARCHAR(128) NOT NULL,TARGET_SYSTEM_ID VARCHAR(128) NOT NULL,TARGET_REPOSITORY_ID VARCHAR(128) NOT NULL,SOURCE_SYSTEM_KIND VARCHAR(128),SOURCE_REPOSITORY_KIND VARCHAR(128),TARGET_SYSTEM_KIND VARCHAR(128),TARGET_REPOSITORY_KIND VARCHAR(128),SOURCE_ARTIFACT_ID VARCHAR(50) NOT NULL,TARGET_ARTIFACT_ID VARCHAR(50) NOT NULL,SOURCE_LAST_MODIFICATION_TIME TIMESTAMP,TARGET_LAST_MODIFICATION_TIME TIMESTAMP,SOURCE_ARTIFACT_VERSION VARCHAR(10),TARGET_ARTIFACT_VERSION VARCHAR(10),ARTIFACT_TYPE VARCHAR(128) NOT NULL,DEP_CHILD_SOURCE_ARTIFACT_ID VARCHAR(128),DEP_CHILD_SOURCE_REPOSITORY_ID VARCHAR(128),DEP_CHILD_SOURCE_REPOSITORY_KIND VARCHAR(128),DEP_CHILD_TARGET_ARTIFACT_ID VARCHAR(128),DEP_CHILD_TARGET_REPOSITORY_ID VARCHAR(128),DEP_CHILD_TARGET_REPOSITORY_KIND VARCHAR(128),DEP_PARENT_SOURCE_ARTIFACT_ID VARCHAR(128),DEP_PARENT_SOURCE_REPOSITORY_ID VARCHAR(128),DEP_PARENT_SOURCE_REPOSITORY_KIND VARCHAR(128),DEP_PARENT_TARGET_ARTIFACT_ID VARCHAR(128),DEP_PARENT_TARGET_REPOSITORY_ID VARCHAR(128),DEP_PARENT_TARGET_REPOSITORY_KIND VARCHAR(128),PRIMARY KEY(SOURCE_SYSTEM_ID,SOURCE_REPOSITORY_ID,TARGET_SYSTEM_ID,TARGET_REPOSITORY_ID,SOURCE_ARTIFACT_ID,ARTIFACT_TYPE))
CREATE INDEX IDENTITY_MAPPING_INDEX ON IDENTITY_MAPPING(SOURCE_SYSTEM_ID,SOURCE_REPOSITORY_ID,TARGET_SYSTEM_ID,TARGET_REPOSITORY_ID,SOURCE_ARTIFACT_ID,ARTIFACT_TYPE)
CREATE MEMORY TABLE ARTIFACTS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,TITLE VARCHAR(60),DESCRIPTION VARCHAR(256),GROUP_VAL VARCHAR(10),STATUS VARCHAR(10),CATEGORY VARCHAR(25),CUSTOMER VARCHAR(25),PRIORITY INTEGER,ASSIGNED_TO VARCHAR(25),REPORTED_IN_RELEASE VARCHAR(25),ESTIMATED_HOURS INTEGER,COMMENT_TEXT VARCHAR(256))
CREATE MEMORY TABLE HOSPITAL(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,TIMESTAMP VARCHAR(128),EXCEPTION_CLASS_NAME VARCHAR(128),EXCEPTION_MESSAGE VARCHAR(128),CAUSE_EXCEPTION_CLASS_NAME VARCHAR(128),CAUSE_EXCEPTION_MESSAGE VARCHAR(128),STACK_TRACE VARCHAR(65536),ADAPTOR_NAME VARCHAR(128),ORIGINATING_COMPONENT VARCHAR(128),DATA_TYPE VARCHAR(128),DATA VARCHAR(256),FIXED BOOLEAN,REPROCESSED BOOLEAN,SOURCE_SYSTEM_ID VARCHAR(128),SOURCE_REPOSITORY_ID VARCHAR(128),TARGET_SYSTEM_ID VARCHAR(128),TARGET_REPOSITORY_ID VARCHAR(128),SOURCE_SYSTEM_KIND VARCHAR(128),SOURCE_REPOSITORY_KIND VARCHAR(128),TARGET_SYSTEM_KIND VARCHAR(128),TARGET_REPOSITORY_KIND VARCHAR(128),SOURCE_ARTIFACT_ID VARCHAR(50),TARGET_ARTIFACT_ID VARCHAR(50),ERROR_CODE VARCHAR(128),SOURCE_LAST_MODIFICATION_TIME TIMESTAMP,TARGET_LAST_MODIFICATION_TIME TIMESTAMP,SOURCE_ARTIFACT_VERSION VARCHAR(10),TARGET_ARTIFACT_VERSION VARCHAR(10),ARTIFACT_TYPE VARCHAR(128),GENERIC_ARTIFACT VARCHAR(6553600))
CREATE INDEX HOSPITAL_INDEX ON HOSPITAL(SOURCE_SYSTEM_ID,SOURCE_REPOSITORY_ID,TARGET_SYSTEM_ID,TARGET_REPOSITORY_ID,SOURCE_ARTIFACT_ID,ARTIFACT_TYPE)
ALTER TABLE ARTIFACTS ALTER COLUMN ID RESTART WITH 6
ALTER TABLE HOSPITAL ALTER COLUMN ID RESTART WITH 75
