<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ccfCoreProperties>
	<!-- 
		value, name, labelName and systemkind are currently mandatory attribute for ccfcoreproperty element.
			name and value is key-value for the properties
			systemKind currently represents QC/SWP 
			lableName is the display name for connector properties in JSP
		 
		direction attribute is not required; property without direction attribute is considered for both direction- FORWARD/REVERSE   
			Example :<ccfcoreproperty category="coreSettings" systemKind="QC" value="1000" name="ccf.direction.sleepinterval" labelName="SleepInterval"/>
			
		category attribute is not required; property without category is considered by default as Miscellaneous category
			Example :<ccfcoreproperty systemKind="QC" value="1000" name="ccf.direction.sleepinterval" labelName="SleepInterval"/>
	-->
	<!-- QC config properties -->
	<ccfcoreproperty category="coreSettings" systemKind="QC" value="1000" name="ccf.direction.sleepinterval" labelName="SleepInterval"/>
	<ccfcoreproperty category="coreSettings" systemKind="QC" value="21600" name="ccf.direction.autorestartperiod" labelName="AutoRestartPeriod"/>
	<ccfcoreproperty category="coreSettings" systemKind="QC" value="true" name="ccf.direction.ship.attachments" labelName="ShipAttachments"/>	
	<ccfcoreproperty category="repoSettings" systemKind="QC" direction="FORWARD" value="false" name="ccf.direction.tf.translate.technical.releaseids" labelName="TranslateTechnicalReleaseIds" />
	<ccfcoreproperty category="repoSettings" systemKind="QC" direction="FORWARD" value="true" name="ccf.direction.tf.releaseIdfields.containfilerelease.packagename" labelName="ReleaseIdFieldsPackageName" />
	<ccfcoreproperty category="repoSettings" systemKind="QC" direction="FORWARD" value="false" name="ccf.direction.tf.retrieve.ship.planningfolder.humanreadablename" labelName="ShipPlanningFolderHumanReadableName" />
	<ccfcoreproperty category="repoSettings" systemKind="QC" direction="FORWARD" value="false" name="ccf.direction.tf.retrieve.ship.release.humanreadablename" labelName="ShipReleaseHumanReadableName" />
	<ccfcoreproperty category="repoSettings" systemKind="QC" direction="FORWARD" value=">" name="ccf.direction.tf.retrieve.planningfolder.separator" labelName="PlanningFolderSeparatorString" />
	<ccfcoreproperty category="repoSettings" systemKind="QC" direction="FORWARD" value=">" name="ccf.direction.tf.retrieve.package.release.separator" labelName="PackageReleaseSeparatorString" />	
	<ccfcoreproperty category="repoSettings" systemKind="QC" direction="REVERSE" value="0"	name="ccf.direction.qc.delaybefore.attachment.download" labelName="DelayBeforeAttachmentDownload" />
	
	<!-- SWP config properties -->
	<ccfcoreproperty category="coreSettings" systemKind="SWP" value="8" name="ccf.direction.identityMapEventWaitCount" labelName="IdentityMapEventWaitCount"/>
	<ccfcoreproperty category="coreSettings" systemKind="SWP" value="500" name="ccf.direction.identityMapEventWaitTime" labelName="IdentityMapEventWaitTime"/>
	<ccfcoreproperty category="coreSettings" systemKind="SWP" value="21600" name="ccf.direction.autorestartperiod" labelName="AutoRestartPeriod"/>	
	<ccfcoreproperty category="coreSettings" systemKind="SWP" direction="FORWARD" value="1000" name="ccf.direction.tf.sleepinterval" labelName="SleepInterval"/>	
	<ccfcoreproperty category="coreSettings" systemKind="SWP" direction="REVERSE" value="250" name="ccf.direction.swp.sleepinterval" labelName="SleepInterval"/>
	
</ccfCoreProperties>